{
  "name": "app-handshake",
  "version": "0.4.2",
  "description": "Devebot oidc authentication layerware",
  "license": "MIT",
  "homepage": "https://github.com/apporo/app-handshake.git",
  "author": {
    "name": "apporo",
    "email": "apporo@devebot.com",
    "url": "https://github.com/apporo"
  },
  "main": "index.js",
  "files": [
    "bin/",
    "config/",
    "data/",
    "lib/",
    "manifest.js"
  ],
  "scripts": {
    "bootstrap": "npm install && cd test && npm install",
    "clean-modules": "rm -rf ./node_modules && rm ./package-lock.json",
    "build": "babel ./src -d ./lib",
    "clean": "rm -rf ./lib",
    "prepublish": "npm run clean && cross-env NODE_ENV=${DEVEBOT_NODE_ENV:-production} npm run build",
    "pretest": "npm run clean && cross-env NODE_ENV=${DEVEBOT_NODE_ENV:-test} npm run build",
    "test": "cd test && npm exec -- mocha --recursive ./**/*-test.js",
    "verify": "npm run prepublish && npm run test",
    "eslint": "npm exec -- eslint ./src/ --quiet"
  },
  "keywords": [
    "devebot",
    "devebot-plugin"
  ],
  "peerDependencies": {
    "app-datastore": "^0.4.0",
    "app-errorlist": "^0.4.0",
    "app-restfetch": "^0.4.0",
    "app-restfront": "^0.4.0",
    "app-tracelog": "^0.4.0",
    "app-webweaver": "^0.4.0",
    "devebot": "^0.4.0"
  },
  "dependencies": {
    "app-datastore": "^0.4.0",
    "app-errorlist": "^0.4.0",
    "app-restfetch": "^0.4.0",
    "app-restfront": "^0.4.0",
    "app-tracelog": "^0.4.0",
    "app-webweaver": "^0.4.0",
    "bcryptjs": "2.4.3",
    "devebot": "^0.4.0",
    "google-libphonenumber": "3.2.13",
    "jsonwebtoken": "8.5.1",
    "moment": "2.29.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "babel-plugin-minify-mangle-names": "^0.5.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0"
  }
}
